{
  "name": "vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "commit": "git-cz",
    "lint": "vue-cli-service lint --no-fix",
    "lint-fix": "vue-cli-service lint --fix",
    "stylelint": "stylelint src/**/*.{html,vue,css,sass,scss} --no-fix",
    "stylelint-fix": "stylelint src/**/*.{html,vue,css,sass,scss} --fix",
    "commitlint": "commitlint --edit",
    "release": "standard-version",
    "initChangelog": "commitizen init cz-conventional-changelog --save-dev --save-exact --force",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w"
  },
  "dependencies": {
    "axios": "0.21.1",
    "core-js": "3.6.5",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "autoprefixer": "8.0.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "compression-webpack-plugin": "5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-vue": "7.9.0",
    "husky": "^6.0.0",
    "less": "4.1.1",
    "less-loader": "7.0.0",
    "lint-staged": "11.0.0",
    "standard-version": "^9.3.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0",
    "svg-sprite-loader": "6.0.6",
    "vue-template-compiler": "2.6.11"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "yarn lint"
    ],
    "src/**/*.{html,vue,css,sass,scss}": [
      "yarn stylelint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
